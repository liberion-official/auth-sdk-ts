{
  "name": "@trust-proto/auth-node",
  "version": "0.2.0",
  "description": "Liberion Auth Backend SDK with post-quantum cryptography",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Liberion Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liberion-official/auth-sdk-ts.git",
    "directory": "packages/backend"
  },
  "homepage": "https://github.com/liberion-official/auth-sdk-ts/tree/main/packages/backend#readme",
  "bugs": {
    "url": "https://github.com/liberion-official/auth-sdk-ts/issues"
  },
  "keywords": [
    "liberion",
    "auth",
    "authentication",
    "websocket",
    "post-quantum",
    "ml-kem",
    "ml-dsa"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@noble/post-quantum": "^0.5.2",
    "ethers": "^6.16.0",
    "libsodium-wrappers": "^0.7.15",
    "uuid": "^13.0.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/libsodium-wrappers": "^0.7.14",
    "@types/node": "^24.10.1",
    "@types/ws": "^8.5.0",
    "@vitest/coverage-v8": "^4.0.15",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.15"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "pnpm run build"
  },
  "publishConfig": {
    "access": "public"
  }
}
